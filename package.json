{
  "name": "webpack-quickstart",
  "version": "1.0.0",
  "description": "Quickstart for creating React apps",
  "author": "Dean Verleger",
  "license": "MIT",
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && yarn && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "yarn run lint -- --watch",
    "clean-dist": "yarn run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "yarn run clean-dist && yarn run lint && yarn run test",
    "build": "babel-node tools/build.js && yarn run open:dist",
    "test": "jest",
    "test:cover": "yarn run test -- --coverage ",
    "test:watch": "yarn run test -- --watch",
    "open:cover": "yarn run test:cover && babel-node ./tools/cover.js",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.22.0",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.15",
    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.12.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.2.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "open": "^0.0.5",
    "postcss-loader": "^1.3.3",
    "prompt": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "replace": "^0.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "uglifyjs-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "yarn-run-all": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deanbot/webpack-quickstart"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    }
  }
}